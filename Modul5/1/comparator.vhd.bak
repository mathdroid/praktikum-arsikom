-- Deskripsi : Program Comparator

ENTITY comparator IS
  PORT (
    D_1 : IN STD_LOGIC_VECTOR (31 DOWNTO 0);
    D_2 : IN STD_LOGIC_VECTOR (31 DOWNTO 0);
    EQ : OUT STD_LOGIC -- Hasil Perbandingan EQ
  );
END comparator;

ARCHITECTURE comp OF comparator IS
  BEGIN
  PROCESS (D_1,D_2)
		BEGIN
			IF (D_1 = D_2) THEN
				EQ <= '1';
			ELSE 
				EQ <= '0';
			END IF;
		END PROCESS;
  END comp;